{
  "expo": {
    "name": "lancelot-client",
    "slug": "lancelot-client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "your-app-scheme",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ibragimlol.lancelot-client"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ibragimlol.lancelotclient"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": ["expo-router"],
    "extra": {
      "eas": {
        "projectId": "f5f17a3e-ff17-40a0-8df5-78d74635917e"
      }
    },
    "owner": "ibragimlol"
  }
}
